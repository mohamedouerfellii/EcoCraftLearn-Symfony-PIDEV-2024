        <div class="section section-padding">

          
            <div class="container">

                            {% for label, messages in app.flashes %}
                {% for message in messages %}
                    <div class="alert alert-danger" style="color: #721c24; background-color: #f8d7da; border-color: #f5c6cb; position: fixed; top: 50%; left: 50%; transform: translate(-50%, -50%);">
                        {{ message }}
                    </div>
                    <script>
                        setTimeout(function() {
                            document.querySelector('.alert').style.display = 'none';
                        }, 3000); 
                    </script>
                {% endfor %}
            {% endfor %}
       {% block bodyProduct %}
                <!-- Courses Category Wrapper Start  -->
                <div class="courses-category-wrapper">
                    <div class="courses-search search-2">
                        <input type="text" placeholder="Search here">
                        <button><i class="icofont-search"></i></button>
                    </div>

                    <ul class="category-menu">
                     
                    <li><a href="{{path ('AddProduct')}}">Add New</a></li>
                        <li><a href="#">Top Rated</a></li>
                        <li><a href="#">Winner </a></li>
                    </ul>
                </div>
                <!-- Courses Category Wrapper End  -->
                <!-- Courses Wrapper Start  -->
                <div class="courses-wrapper-02">
            
                    <div class="row">
                        {% for p in Products %}
                        <div class="col-lg-4 col-md-6">
                            <!-- Single Courses Start -->
                          
                            <div class="single-courses">
                                <div class="courses-images">
                                    <a href="{{ path('showProductsDetail', { 'idproduct': p.idproduct }) }}"><img src="{{ asset('contents/uploads/products/' ~ p.image) }}" alt="Courses" style="width:330px;height:300px;"></a>
                                </div>
                                <div class="courses-content">
                                    <div class="courses-author">
                                        <div class="author">
                                    
                            
                                        </div>
                                    </div>

                                    <h4 class="title"><a href="">{{ p.name }}</a></h4>

                                   
                                </div>
                            </div>
                            <!-- Single Courses End -->
                        </div>
                        {% endfor %}
                       
                       
                      

                        </div>
                    </div>
                      {% endblock %}
