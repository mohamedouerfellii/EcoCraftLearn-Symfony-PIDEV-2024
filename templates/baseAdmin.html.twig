<body>
    <div class="main-wrapper main-wrapper-02">
        <!-- Login Header Start -->
        <div class="section login-header">
            <!-- Login Header Wrapper Start -->
            <div class="login-header-wrapper navbar navbar-expand">
               <!-- Header Logo Start -->
                <div id="urlNotify" class="login-header-logo logo-2" data-url="{{ path('notify_tutor') }}">
                    <a id="idTutor" data-id-tutor="{{ app.user.iduser }}"  {% block linkHome %} href="{{path('tutor_course_dashboard')}}" {% endblock %}><img  src="{{asset('assets/images/customLogo.png')}}" alt="Logo"></a></li>
                </div>
                    <script>
                const idTutor = document.getElementById('idTutor').getAttribute('data-id-tutor');
                    document.addEventListener('DOMContentLoaded', function() {
                        notifytutor(idTutor,false);
                    });
                </script>
                <!-- Header Logo End -->
                <!-- Header Search Start -->
                <div class="login-header-search dropdown">
                    <button class="search-toggle" data-bs-toggle="dropdown"><i class="flaticon-loupe"></i></button>

                    <div class="search-input dropdown-menu">
                        {% block search %}
                            <input type="text" placeholder="Search here">
                        {% endblock %}
                    </div>

                </div>
                <!-- Header Search End -->

                <!-- Header Action Start -->
                <div class="login-header-action ml-auto">
                {% block notification %}
                    <div class="dropdown">
                        <button class="action notification" data-bs-toggle="dropdown">
                            <i class="flaticon-notification"></i>
                            <span id="notifAlert"></span>
                        </button>
                        <div class="dropdown-menu dropdown-notification">
                            <ul id="notifList" class="notification-items-list">
                                
                            </ul>
                            <a class="all-notification" href="{{ path('read_notification_course',{'idTutor': app.user.iduser})}}">See all notifications</a>
                        </div>
                    </div>
                    {% endblock %}
                      {% if app.user %}
    {# User is logged in, so hide the sign-in and sign-up links #}
    

    <a class="action author" href="#">
                      {% if app.user.image starts with "https" %}
                        <img src="{{ app.user.image }}" alt="Author">
                    {% else %}
                        <img src="{{ asset('contents/uploads/users/' ~ app.user.image) }}" alt="Author">
                    {% endif %}
                    </a>

                    <div class="dropdown">
                        <button class="action more" data-bs-toggle="dropdown">
                            <span></span>
                            <span></span>
                            <span></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a class="" href="#"><i class="icofont-user"></i> Profile</a></li>
                            <li><a class="" href="#"><i class="icofont-inbox"></i> Inbox</a></li>
                            <li><a class="" href="{{ path('app_logout') }}"><i class="icofont-logout"></i> Sign Out</a></li>
                        </ul>
                    </div>
{% else %}
    {# User is not logged in, so show the sign-in and sign-up links #}
    <div class="header-sign-in-up d-none d-lg-block">
        <ul>
            <li><a class="sign-in" href="{{ path('app_login') }}">Sign In</a></li>
            <li><a class="sign-up" href="{{ path('app_register') }}">Sign Up</a></li>
        </ul>
    </div>

                    
    {% endif %}
                </div>
                <!-- Header Action End -->

            </div>
            <!-- Login Header Wrapper End -->
        </div>
        <!-- Login Header End -->
                <!-- Courses Admin Start -->
        <div class="section overflow-hidden position-relative" id="wrapper">
                    {% block sideBar %}
            <!-- Sidebar Wrapper Start -->
            <div class="sidebar-wrapper">
                <div class="menu-list">
                    <a class="active" href="{{ path('admin_dashboard')}}"><img src="{{asset('assets/sidebar/home-4-line.png')}}" alt="Icon"></a>
                    <a href="{{ path('showProductsDashboard')}}"><img src="{{asset('assets/sidebar/inbox-line.png')}}" alt="Icon"></a>
                    <a href="{{ path('afficher_collectspts2')}}"><img src="{{asset('assets/sidebar/recycle-line.png')}}" alt="Icon"></a>
                </div>
            </div>
            <!-- Sidebar Wrapper End -->
            {% endblock %}
        <div class="page-content-wrapper">
            <div class="container-fluid custom-container">
            {% block body %}
            {% endblock %}
            <br>
            <br>
            <br>
            </div>
        </div>
        </div>
        <!-- mercure notification -->
        <script>
                let notifDesc = document.getElementById("notifDesc");
                const eventSource = new EventSource("{{ mercure('https://ecocraftlearning/enroll/'~ app.user.iduser)|escape('js') }}");
                eventSource.onmessage = event => {
                    notifData = JSON.parse(event.data);
                    notifytutor(notifData['idTutor'],true);
                }
        </script>
        {% block footer %}
        <!-- Footer Start  -->
        <div class="section footer-section">
            <!-- Footer Copyright Start -->
